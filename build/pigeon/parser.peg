Input <- Term !.

Term <- Variable AndVar*

AndVar <- _ "AND" _ Term

Variable <- FieldChars+ _ ":" _ Value

Value <- '"' ValueChars '"'

FieldChars <- [a-z]

ValueChars <- [a-zA-Z0-9 !]

_ "whitespace" <- [ \n\t\r]*
